{
  "name": "kanji-go-backend",
  "version": "1.0.0",
  "description": "Kanji Go API Server with PostgreSQL and Kanji Alive API integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No tests yet\" && exit 0",
    "db:create": "psql -U postgres -c \"CREATE DATABASE kanji_go_db;\"",
    "db:setup": "psql -U kanji_dev -d kanji_go_db -f ../database/schema/kanji_info_table.sql"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "kanji",
    "learning",
    "api",
    "postgresql"
  ],
  "author": "Kanji Go Team",
  "license": "MIT"
}
